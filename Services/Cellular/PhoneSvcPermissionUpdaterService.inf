[Version]
Signature   = "$WINDOWS NT$"
Class       = SYSTEM
ClassGuid   = {4d36e97d-e325-11ce-bfc1-08002be10318}
Provider    = %IHV%
DriverVer   = 09/09/2015,1.0.0.1
CatalogFile = PhoneSvcPermissionUpdaterService.cat

[DestinationDirs]
